{
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "ES2019",
      "DOM",
      "DOM.Iterable"
    ],
    "moduleResolution": "node",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "jsxImportSource": "lestin",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
  },
}
